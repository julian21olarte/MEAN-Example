    script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js", integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa", crossorigin="anonymous")
    script(src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.6/angular.min.js")
    script(src="https://code.angularjs.org/1.6.6/angular-route.min.js")
    script(src="/javascripts/router.js")
    script(src="/javascripts/factories/alert.factory.js")
    script(src="/javascripts/controllers/index.controller.js")
    script(src="/javascripts/controllers/users.controller.js")
    script(src="/javascripts/controllers/user.controller.js")
    script(src="/javascripts/controllers/user.add.controller.js")
